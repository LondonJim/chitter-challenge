<%= flash[:sign_up_notice]%>
<% if @current_user != nil %>
  <%= @current_user.user_name %> is currently logged in </br>
<% else %>
  no one is currently logged in
<% end %>

<form action="/sign_up" method="get">
  <input type="submit" value="SIGN UP" />
</form>

<form action="/log_in" method="get">
  <input type="submit" value="LOG IN" />
</form>

<form action="/log_out" method="get">
  <input type="submit" value="LOG OUT" />
</form>


<form action="/peep_feed" method="get">
  <input type="submit" value="VIEW PEEPS" />
</form>
